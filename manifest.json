{
    "manifest_version": 3,
    "name": "Notice Board Extension",
    "version": "1.0",
    "permissions": ["activeTab","cookies"
  ],

    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon.jpeg"
      }
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  